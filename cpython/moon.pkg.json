{
  "is-main": false,
  "import": [
    { "path": "moonbitlang/core/encoding/utf8", "alias": "encoding/utf8" },
    "moonbitlang/core/bytes"
  ],
  "supported-targets": ["native"],
  "native-stub": ["wrap.c"],
  "link": {
    "native": {
      "cc": "${build.CC}",
      "cc-flags": "${build.CC_FLAGS}",
      "cc-link-flags": "${build.CC_LINK_FLAGS}",
      "stub-cc": "${build.CC}",
      "stub-cc-flags": "${build.STUB_CC_FLAGS}",
      "stub-cc-link-flags": "${build.STUB_CC_LINK_FLAGS}"
    }
  }
}
